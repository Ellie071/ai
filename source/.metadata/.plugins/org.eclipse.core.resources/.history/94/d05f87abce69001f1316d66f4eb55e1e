package com.lec.ex4_product;
// new product("S22")
public class Product {
	private String mode1No; //모델명
	int serialNo; // 객체 교유의 번호
	private static int count = 100; //
	public Product(String mode1No) {
		this.mode1No = mode1No;
		serialNo = ++count;
	}
	public void infoPrint() {
		System.out.println("모델명 : " + mode1No + "");
	}
}
